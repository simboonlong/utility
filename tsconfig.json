{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "noImplicitThis": false, // for skipping throtte function
    "rootDir": "src"
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
